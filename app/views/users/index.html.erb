<div class="wrapper">
   <h3>List of All Current Employees:</h3>
   <div class="user-list">
      <table>
      <tr>
        <th>Username</th>
      </tr>
      <% @users.each do |user| %>
        <tr>
          <td><%=user.username %></td>
        	<td><span id="link-tag"><%= link_to "Show", user_path(user) %></span></td>
        	<td><span id="link-tag"><%= link_to "Edit", edit_user_path(user) %></span></td>
        	<td><span id="link-tag"><%= link_to "Delete", user_path(user) %></span></td>
        </tr>
      <% end %>
      </table>
    </div>
</div>

    
