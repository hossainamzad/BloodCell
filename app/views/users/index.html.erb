 <h3>Listing All Current Users:</h3>
 
<table>
  <tr>
    <th>Username</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%=user.username %></td>
    	<td><%= link_to "Show", user_path(user) %></td>
    	<td><%= link_to "Edit", edit_user_path(user) %></td>
    	<td><%= link_to "Delete", user_path(user) %></td>
    </tr>
  <% end %>
</table>

    
